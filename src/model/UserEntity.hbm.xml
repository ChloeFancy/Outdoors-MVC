<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.UserEntity" table="user" schema="outdoors">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="mail">
            <column name="mail" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="tel">
            <column name="tel" sql-type="char(11)" length="11"/>
        </property>
        <property name="password">
            <column name="password" sql-type="char(32)" length="32" not-null="true"/>
        </property>
        <property name="photoPath">
            <column name="photoPath" sql-type="varchar(50)" length="50" />
        </property>
    </class>
</hibernate-mapping>